{
  "name": "TrailerZoo",
  "description": "Lightning fast movie trailers",
  "author": "Ben Shyong <hello@benshyong.com> (http://github.com/bshyong)",
  "version": "0.0.1",
  "homepage": "http://trailerzoo.com",
  "keywords": [
    "react",
    "redux"
  ],
  "main": "babel.server.js",
  "scripts": {
    "start": "NODE_PATH=\"./src\" NODE_ENV=\"production\" node ./babel.server",
    "build": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config --progress webpack/prod.config.js",
    "start-dev": "NODE_PATH=\"./src\" NODE_ENV=\"development\" node ./babel.server",
    "watch-client": "NODE_PATH=\"./src\" node webpack/webpack-dev-server.js",
    "dev": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "babel": "5.4.7",
    "babel-plugin-typecheck": "0.0.3",
    "basscss": "^6.1.6",
    "compression": "^1.5.0",
    "css-loader": "^0.15.1",
    "express": "^4.13.0",
    "express-session": "^1.11.3",
    "file-loader": "^0.8.4",
    "http-proxy": "^1.11.1",
    "piping": "0.1.8",
    "react": "0.13.3",
    "react-inline-css": "1.1.1",
    "react-router": "v1.0.0-beta2",
    "redux": "^0.12.0",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0",
    "superagent": "^1.2.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.11",
    "extract-text-webpack-plugin": "^0.3.1",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.12.3",
    "autoprefixer-loader": "^2.0.0",
    "babel-runtime": "5.4.7",
    "babel-loader": "5.1.3",
    "babel-core": "5.4.7",
    "babel-eslint": "^3.1.18"
  },
  "devDependencies": {
    "concurrently": "0.1.1",
    "eslint": "^0.23.0",
    "eslint-config-airbnb": "0.0.6",
    "eslint-plugin-react": "^2.5.2",
    "json-loader": "0.5.2",
    "node-sass": "^3.2.0",
    "react-a11y": "0.1.1",
    "react-hot-loader": "1.2.7",
    "react-style-webpack-plugin": "^0.4.0",
    "sass-loader": "^1.0.2",
    "webpack-dev-server": "1.9.0"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
